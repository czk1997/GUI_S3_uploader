<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.GridPane?>

<GridPane prefHeight="600.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
  <children>
    <Pane prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="0" GridPane.rowIndex="0">
      <children>
        <TextArea fx:id="infoArea" editable="false" layoutX="14.0" layoutY="46.0" prefHeight="370.0" prefWidth="369.0" wrapText="true" />
        <TextField fx:id="infoField" layoutX="16.0" layoutY="451.0" onMouseClicked="#infoLinster" prefHeight="34.9609375" prefWidth="369.0" text="Click me to select the file." />
        <Button fx:id="submit" layoutX="261.0" layoutY="533.0" mnemonicParsing="false" onAction="#submitLinster" prefHeight="34.9609375" prefWidth="115.0" text="Submit" />
        <Label layoutX="14.0" layoutY="4.0" prefHeight="34.9609375" prefWidth="46.0" text="Info" />
        <Label layoutX="14.0" layoutY="432.0" text="File Path" />
        <ProgressBar fx:id="progressBar" layoutX="17.0" layoutY="486.0" prefHeight="34.9609375" prefWidth="369.0" progress="0.0" />
            <Button fx:id="StopButton" layoutX="54.0" layoutY="533.0" mnemonicParsing="false" onAction="#stopButttonLinister" prefHeight="34.9609375" prefWidth="115.0" text="Stop" />
      </children>
    </Pane>
  </children>
  <columnConstraints>
    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
  </columnConstraints>
  <rowConstraints>
    <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
  </rowConstraints>
</GridPane>
